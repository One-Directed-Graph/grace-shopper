    "express": "^4.16.4",
    "express-session": "^1.17.1",
    "font-awesome": "^4.7.0",
    "history": "^4.10.1",
    "imagesloaded": "^4.1.4",
    "jquery": "^3.6.0",
    "jquery-countdown": "^2.2.0",
    "magnific-popup": "^1.1.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "owl.carousel": "^2.3.4",
    "parallax-js": "^3.1.0",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.2",
    "popper.js": "^1.16.1",
